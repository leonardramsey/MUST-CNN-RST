                ( Satellite (span 19 21) (rel2par attribution)
                  ( Nucleus (span 19 20) (rel2par Same-Unit)
                    ( Nucleus (leaf 19) (rel2par span) (text _!A company spokesman,_!) )
                    ( Satellite (leaf 20) (rel2par elaboration-additional-e) (text _!reached at his home last night,_!) )
                  )
                  ( Nucleus (leaf 21) (rel2par Same-Unit) (text _!would only say_!) )
                )
                ( Nucleus (leaf 22) (rel2par span) (text _!that there will be a public announcement of the reduction program by the end of the week.<P>_!) )
              )
            )
