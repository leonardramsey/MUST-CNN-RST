                  ( Nucleus (span 17 18) (rel2par span)
                    ( Nucleus (leaf 17) (rel2par List) (text _!that "the restructuring will take place over a two-year period_!) )
                    ( Nucleus (leaf 18) (rel2par List) (text _!and will principally involve the transfer and termination of employees in our U.S. operations."<P>_!) )
                  )
                )
              )
