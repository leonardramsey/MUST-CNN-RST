                            ( Satellite (span 171 172) (rel2par elaboration-additional)
                              ( Satellite (leaf 171) (rel2par circumstance) (text _!When the pilots refused,_!) )
                              ( Nucleus (leaf 172) (rel2par span) (text _!the company retracted it.<P>_!) )
