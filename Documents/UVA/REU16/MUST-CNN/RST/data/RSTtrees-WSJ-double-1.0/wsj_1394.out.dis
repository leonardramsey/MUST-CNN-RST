                )
                ( Satellite (span 154 205) (rel2par elaboration-additional)
                  ( Nucleus (span 154 155) (rel2par span)
                    ( Satellite (leaf 154) (rel2par concession) (text _!Although encouraging dialogue between managers and workers,_!) )
                    ( Nucleus (leaf 155) (rel2par span) (text _!Mr. Smith doesn't countenance what he considers insubordination._!) )
                  )
                  ( Satellite (span 156 205) (rel2par example)
                    ( Nucleus (span 156 162) (rel2par span)
                      ( Satellite (span 156 160) (rel2par concession)
                        ( Satellite (span 156 157) (rel2par circumstance)
                          ( Nucleus (leaf 156) (rel2par span) (text _!When a large group of pilots once signed petitions_!) )
                          ( Satellite (leaf 157) (rel2par elaboration-object-attribute-e) (text _!opposing work-rule and compensation changes,_!) )
                        )
                        ( Nucleus (span 158 160) (rel2par span)
