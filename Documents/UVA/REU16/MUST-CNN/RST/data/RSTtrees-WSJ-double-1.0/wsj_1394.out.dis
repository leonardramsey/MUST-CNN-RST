          ( Nucleus (span 146 148) (rel2par span)
            ( Nucleus (span 146 147) (rel2par span)
              ( Nucleus (leaf 146) (rel2par span) (text _!Analysts expect Federal's earnings to improve again in its fiscal third quarter_!) )
              ( Satellite (leaf 147) (rel2par elaboration-object-attribute-e) (text _!ending Feb. 28,_!) )
            )
            ( Satellite (leaf 148) (rel2par circumstance) (text _!when the company should begin benefiting from Tiger's extra flights, back-up planes and landing rights._!) )
