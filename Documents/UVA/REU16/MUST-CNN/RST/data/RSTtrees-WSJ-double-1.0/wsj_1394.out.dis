                                            ( Nucleus (leaf 200) (rel2par span) (text _!Furthermore,_!) )
                                            ( Satellite (leaf 201) (rel2par attribution-e) (text _!Mr. Smith added,_!) )
                                          )
                                          ( Nucleus (leaf 202) (rel2par Same-Unit) (text _!"our contract with our pilots says_!) )
                                        )
                                        ( Satellite (leaf 203) (rel2par attribution) (text _!that we will manage our fleet operations with their advice._!) )
                                      )
                                      ( Satellite (span 204 205) (rel2par elaboration-additional)
                                        ( Nucleus (leaf 204) (rel2par span) (text _!It doesn't give any particular group the ability_!) )
                                        ( Satellite (leaf 205) (rel2par elaboration-object-attribute-e) (text _!to veto change."<P>_!) )
