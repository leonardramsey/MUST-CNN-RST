                              ( Satellite (span 183 184) (rel2par elaboration-additional)
                                ( Satellite (leaf 183) (rel2par attribution) (text _!The Tiger merger agreement stipulated_!) )
                                ( Nucleus (leaf 184) (rel2par span) (text _!that the lists be combined on the basis of tenure.<P>_!) )
