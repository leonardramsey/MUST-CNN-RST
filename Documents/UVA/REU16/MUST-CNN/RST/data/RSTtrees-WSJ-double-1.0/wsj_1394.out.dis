                          ( Satellite (span 223 235) (rel2par evaluation-s)
                            ( Nucleus (span 223 225) (rel2par span)
                              ( Satellite (leaf 223) (rel2par result) (text _!The fight has turned ugly_!) )
                              ( Nucleus (span 224 225) (rel2par span)
                                ( Nucleus (leaf 224) (rel2par span) (text _!and, among pilots at least, has shattered the esprit de corps_!) )
                                ( Satellite (leaf 225) (rel2par elaboration-object-attribute-e) (text _!that Mr. Smith worked so hard to build._!) )
                              )
                            )
                            ( Satellite (span 226 235) (rel2par explanation-argumentative)
                              ( Nucleus (leaf 226) (rel2par List) (text _!Anti-union pilots have held ballot-burning parties._!) )
                              ( Nucleus (span 227 229) (rel2par List)
                                ( Satellite (leaf 227) (rel2par attribution) (text _!Some younger pilots say_!) )
                                ( Nucleus (span 228 229) (rel2par span)
                                  ( Nucleus (leaf 228) (rel2par span) (text _!they have had to endure anti-union harangues by senior pilots_!) )
                                  ( Satellite (leaf 229) (rel2par circumstance) (text _!while flying across the country.<P>_!) )
                                )
                              )
                              ( Nucleus (span 230 235) (rel2par List)
                                ( Nucleus (leaf 230) (rel2par span) (text _!And for now, at least, the competition isn't the only enemy._!) )
                                ( Satellite (span 231 235) (rel2par explanation-argumentative)
                                  ( Nucleus (span 231 233) (rel2par span)
                                    ( Satellite (leaf 231) (rel2par attribution) (text _!Barney Barnhardt, a 727 captain and leader of the pro-union forces, said_!) )
                                    ( Nucleus (span 232 233) (rel2par span)
                                      ( Nucleus (leaf 232) (rel2par List) (text _!he has received two anonymous death threats_!) )
                                      ( Nucleus (leaf 233) (rel2par List) (text _!and been challenged to a fight with tire irons by a colleague._!) )
                                    )
                                  )
                                  ( Satellite (span 234 235) (rel2par elaboration-additional)
