                          ( Nucleus (span 164 167) (rel2par span)
                            ( Nucleus (span 164 166) (rel2par span)
                              ( Nucleus (span 164 165) (rel2par Same-Unit)
                                ( Nucleus (leaf 164) (rel2par span) (text _!Its 6,500 workers,_!) )
                                ( Satellite (leaf 165) (rel2par elaboration-additional-e) (text _!who had battled Tiger's management for years over givebacks,_!) )
                              )
                              ( Nucleus (leaf 166) (rel2par Same-Unit) (text _!were union members until the day of the merger,_!) )
                            )
                            ( Satellite (leaf 167) (rel2par circumstance) (text _!when most of their unions were automatically decertified._!) )
