                        )
                        ( Nucleus (span 185 205) (rel2par Problem-Solution)
                          ( Nucleus (span 185 189) (rel2par span)
                            ( Nucleus (leaf 185) (rel2par span) (text _!Mr. Smith is trying hard to allay the anger._!) )
                            ( Satellite (span 186 189) (rel2par elaboration-additional)
                              ( Nucleus (span 186 187) (rel2par span)
                                ( Satellite (leaf 186) (rel2par attribution) (text _!And even some pro-union pilots say_!) )
                                ( Nucleus (leaf 187) (rel2par span) (text _!his charisma and popularity among the many former military fliers could be tough to beat._!) )
                              )
                              ( Satellite (span 188 189) (rel2par explanation-argumentative)
                                ( Nucleus (leaf 188) (rel2par span) (text _!"A lot of people are identifying a vote for representation as a vote against Fred Smith,"_!) )
                                ( Satellite (leaf 189) (rel2par attribution) (text _!says J.X. Gollich, a Tiger-turned-Federal pilot and union activist.<P>_!) )
                              )
                            )
                          )
                          ( Satellite (span 190 205) (rel2par elaboration-additional)
                            ( Nucleus (leaf 190) (rel2par span) (text _!Mr. Smith and other top Federal executives have met with Tiger workers in Los Angeles, Ohio, New York, Alaska, Asia and Europe._!) )
                            ( Satellite (span 191 205) (rel2par elaboration-additional)
                              ( Nucleus (leaf 191) (rel2par span) (text _!Recently, they have appeared every few weeks in talk-show type videos, countering pro-union arguments._!) )
                              ( Satellite (span 192 205) (rel2par elaboration-additional)
                                ( Nucleus (span 192 193) (rel2par span)
                                  ( Nucleus (leaf 192) (rel2par span) (text _!In one video, Mr. Smith defended his agreement_!) )
