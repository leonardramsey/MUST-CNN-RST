              ( Satellite (span 206 235) (rel2par cause)
                ( Nucleus (leaf 206) (rel2par span) (text _!Already, the fight has been costly._!) )
                ( Satellite (span 207 235) (rel2par explanation-argumentative)
                  ( Nucleus (leaf 207) (rel2par span) (text _!The seniority-list controversy, along with the job-classification dispute, has been turned over to the mediator._!) )
                  ( Satellite (span 208 235) (rel2par elaboration-additional)
                    ( Nucleus (span 208 210) (rel2par List)
                      ( Nucleus (span 208 209) (rel2par span)
                        ( Nucleus (leaf 208) (rel2par Cause-Result) (text _!Meanwhile, the company is operating with two separate pilot groups and seniority lists,_!) )
                        ( Nucleus (leaf 209) (rel2par Cause-Result) (text _!and that is costing Federal "a big number,"_!) )
                      )
                      ( Satellite (leaf 210) (rel2par attribution) (text _!says James Barksdale, executive vice president and chief operating officer.<P>_!) )
                    )
                    ( Nucleus (span 211 235) (rel2par List)
                      ( Nucleus (leaf 211) (rel2par span) (text _!The issue has also cost Federal management a lot of good will among its old pilots._!) )
                      ( Satellite (span 212 235) (rel2par explanation-argumentative)
                        ( Nucleus (span 212 216) (rel2par span)
                          ( Nucleus (span 212 214) (rel2par span)
                            ( Nucleus (span 212 213) (rel2par span)
                              ( Nucleus (leaf 212) (rel2par span) (text _!"They were willing to mistreat us_!) )
                              ( Satellite (leaf 213) (rel2par reason) (text _!because we hadn't shown any moxie, any resistance,"_!) )
                            )
                            ( Satellite (leaf 214) (rel2par attribution) (text _!says William Queenan, a DC-10 pilot and 14-year Federal veteran._!) )
                          )
                          ( Satellite (span 215 216) (rel2par elaboration-additional)
                            ( Satellite (leaf 215) (rel2par attribution) (text _!Adds John Poag, a 727 captain and past chairman of the company-sponsored Flight Advisory Board:_!) )
                            ( Nucleus (leaf 216) (rel2par span) (text _!"They've made all these magnanimous gestures to the Flying Tiger pilots, and for us, nothing."<P>_!) )
                          )
                        )
                        ( Satellite (span 217 235) (rel2par elaboration-additional)
                          ( Nucleus (span 217 222) (rel2par span)
                            ( Nucleus (leaf 217) (rel2par span) (text _!Such animosity could prove pivotal in the union vote._!) )
                            ( Satellite (span 218 222) (rel2par explanation-argumentative)
                              ( Satellite (span 218 219) (rel2par antithesis)
                                ( Nucleus (leaf 218) (rel2par span) (text _!A large majority of the 961 former Tiger fliers support the union,_!) )
                                ( Satellite (leaf 219) (rel2par attribution) (text _!according to a union study._!) )
                              )
                              ( Nucleus (span 220 222) (rel2par span)
                                ( Nucleus (leaf 220) (rel2par Same-Unit) (text _!But_!) )
                                ( Nucleus (span 221 222) (rel2par Same-Unit)
                                  ( Satellite (leaf 221) (rel2par concession-e) (text _!though most of the 1,087 Federal pilots are believed opposed,_!) )
                                  ( Nucleus (leaf 222) (rel2par span) (text _!it is unclear just how much their loyalty to Mr. Smith has been eroded.<P>_!) )
                                )
