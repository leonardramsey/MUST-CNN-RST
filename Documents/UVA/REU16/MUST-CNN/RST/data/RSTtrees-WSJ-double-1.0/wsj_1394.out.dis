                              ( Nucleus (span 176 182) (rel2par span)
                                ( Nucleus (span 176 177) (rel2par span)
                                  ( Nucleus (leaf 176) (rel2par span) (text _!Mr. Smith ignored a promise_!) )
                                  ( Satellite (leaf 177) (rel2par elaboration-additional-e) (text _!that he had made to his own pilots three years ago:_!) )
                                )
                                ( Satellite (span 178 182) (rel2par elaboration-object-attribute-e)
                                  ( Nucleus (span 178 179) (rel2par Same-Unit)
                                    ( Nucleus (leaf 178) (rel2par span) (text _!that any fliers_!) )
                                    ( Satellite (leaf 179) (rel2par elaboration-object-attribute-e) (text _!acquired in future mergers_!) )
                                  )
                                  ( Nucleus (span 180 182) (rel2par Same-Unit)
                                    ( Nucleus (leaf 180) (rel2par span) (text _!would be "end-tailed"_!) )
                                    ( Satellite (span 181 182) (rel2par definition)
                                      ( Nucleus (leaf 181) (rel2par span) (text _!-- put at the bottom of the pilot seniority list_!) )
                                      ( Satellite (leaf 182) (rel2par elaboration-object-attribute-e) (text _!that determines work schedules, pay and career options._!) )
                                    )
                                  )
                                )
