                                    ( Nucleus (span 197 199) (rel2par List)
                                      ( Nucleus (span 197 198) (rel2par span)
                                        ( Nucleus (leaf 197) (rel2par span) (text _!Regulators, as well, might have quashed the deal_!) )
                                        ( Satellite (leaf 198) (rel2par condition) (text _!if Tiger's pilots hadn't been protected,_!) )
