                      ( Satellite (span 173 205) (rel2par elaboration-additional)
                        ( Nucleus (span 173 184) (rel2par Problem-Solution)
                          ( Nucleus (leaf 173) (rel2par span) (text _!Mr. Smith angered Federal's pilots, too._!) )
                          ( Satellite (span 174 184) (rel2par explanation-argumentative)
                            ( Nucleus (span 174 175) (rel2par Same-Unit)
                              ( Nucleus (leaf 174) (rel2par span) (text _!In his haste_!) )
                              ( Satellite (leaf 175) (rel2par elaboration-object-attribute-e) (text _!to seal the deal with Tiger Chairman Saul Steinberg last August,_!) )
