                      ( Nucleus (span 120 121) (rel2par Same-Unit)
                        ( Nucleus (leaf 120) (rel2par span) (text _!If the LDP,_!) )
                        ( Satellite (leaf 121) (rel2par circumstance-e) (text _!as currently composed,_!) )
                      )
                      ( Nucleus (leaf 122) (rel2par Same-Unit) (text _!can't make the transformation,_!) )
