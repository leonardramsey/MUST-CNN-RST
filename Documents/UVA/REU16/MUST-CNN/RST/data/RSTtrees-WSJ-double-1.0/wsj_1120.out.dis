                ( Satellite (span 102 105) (rel2par evaluation-n)
                  ( Nucleus (span 102 103) (rel2par Same-Unit)
                    ( Nucleus (leaf 102) (rel2par span) (text _!In the recent Upper House and Tokyo metropolitan congressional elections,_!) )
                    ( Satellite (leaf 103) (rel2par elaboration-additional-e) (text _!in which the Socialist Party won a runaway victory,_!) )
                  )
                  ( Nucleus (span 104 105) (rel2par Same-Unit)
                    ( Nucleus (leaf 104) (rel2par span) (text _!60% of all women voted,_!) )
                    ( Satellite (leaf 105) (rel2par comparison) (text _!as opposed to the usual 40%._!) )
                  )
                )
                ( Nucleus (span 106 108) (rel2par span)
                  ( Satellite (leaf 106) (rel2par antithesis) (text _!It is difficult to analyze how much of their anger was due to Recruit, the sex scandals, or the one-yen coins in their purses,_!) )
                  ( Nucleus (span 107 108) (rel2par span)
                    ( Nucleus (leaf 107) (rel2par span) (text _!but they obviously were voting_!) )
                    ( Satellite (leaf 108) (rel2par purpose) (text _!to punish the LDP.<P>_!) )
                  )
                )
              )
            )
