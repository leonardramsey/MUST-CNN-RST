              )
              ( Satellite (span 128 129) (rel2par elaboration-additional)
                ( Nucleus (leaf 128) (rel2par List) (text _!The LDP must make a decision immediately;_!) )
                ( Nucleus (leaf 129) (rel2par List) (text _!Lower House elections must take place before June._!) )
              )
            )
            ( Satellite (span 130 131) (rel2par elaboration-additional)
              ( Nucleus (leaf 130) (rel2par span) (text _!In the current mood of the Japanese people, journalists and even some industrialists, giving power to the Socialists might be good for the LDP,_!) )
              ( Satellite (leaf 131) (rel2par elaboration-object-attribute-e) (text _!cleansing it of past sins._!) )
