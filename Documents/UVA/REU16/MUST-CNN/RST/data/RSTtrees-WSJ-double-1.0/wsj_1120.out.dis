( Root (span 1 135)
  ( Nucleus (span 1 134) (rel2par TextualOrganization)
    ( Nucleus (span 1 4) (rel2par span)
      ( Satellite (span 1 2) (rel2par antithesis)
        ( Satellite (leaf 1) (rel2par consequence-n) (text _!Japan has climbed up from the ashes of World War II and a gross national product of about $800 per capita_!) )
        ( Nucleus (leaf 2) (rel2par span) (text _!to reach the heavyweight class among industrialized nations._!) )
      )
      ( Nucleus (span 3 4) (rel2par span)
        ( Nucleus (leaf 3) (rel2par span) (text _!Now this remarkable economic growth seems to be coming to an end_!) )
        ( Satellite (leaf 4) (rel2par explanation-argumentative) (text _!because the government has not converted itself into a modern, democratic, "developed nation" mode of operation.<P>_!) )
      )
    )
    ( Satellite (span 5 134) (rel2par summary-n)
      ( Nucleus (span 5 108) (rel2par Problem-Solution)
        ( Nucleus (span 5 94) (rel2par span)
          ( Nucleus (span 5 28) (rel2par span)
            ( Nucleus (span 5 27) (rel2par span)
              ( Satellite (span 5 25) (rel2par background)
                ( Nucleus (span 5 21) (rel2par span)
                  ( Nucleus (span 5 18) (rel2par span)
                    ( Nucleus (span 5 7) (rel2par span)
                      ( Nucleus (span 5 6) (rel2par Same-Unit)
                        ( Nucleus (leaf 5) (rel2par span) (text _!Until 1980,_!) )
                        ( Satellite (leaf 6) (rel2par temporal-same-time-e) (text _!when Japan joined the $10,000 per capita GNP club of the advanced countries,_!) )
                      )
                      ( Nucleus (leaf 7) (rel2par Same-Unit) (text _!it was a model developing nation._!) )
                    )
                    ( Satellite (span 8 18) (rel2par evidence)
                      ( Nucleus (leaf 8) (rel2par List) (text _!The government built ports, bridges, highways, schools, hospitals and railways._!) )
                      ( Nucleus (span 9 10) (rel2par List)
                        ( Satellite (leaf 9) (rel2par circumstance) (text _!When industries were weak,_!) )
                        ( Nucleus (leaf 10) (rel2par span) (text _!it protected them._!) )
                      )
                      ( Nucleus (span 11 18) (rel2par List)
                        ( Nucleus (leaf 11) (rel2par span) (text _!It gave the Japanese people a value system,_!) )
                        ( Satellite (span 12 18) (rel2par elaboration-object-attribute-e)
                          ( Nucleus (leaf 12) (rel2par span) (text _!based on the rationalization_!) )
                          ( Satellite (span 13 18) (rel2par elaboration-object-attribute-e)
                            ( Nucleus (span 13 17) (rel2par span)
                              ( Nucleus (leaf 13) (rel2par Same-Unit) (text _!that_!) )
                              ( Nucleus (span 14 17) (rel2par Same-Unit)
                                ( Satellite (leaf 14) (rel2par circumstance-e) (text _!given the country's lack of natural resources,_!) )
                                ( Nucleus (span 15 17) (rel2par span)
                                  ( Nucleus (span 15 16) (rel2par List)
                                    ( Nucleus (leaf 15) (rel2par span) (text _!they must work hard_!) )
                                    ( Satellite (leaf 16) (rel2par purpose) (text _!to create value through exports_!) )
                                  )
                                  ( Nucleus (leaf 17) (rel2par List) (text _!and buy food with the surplus._!) )
                                )
                              )
                            )
                            ( Satellite (leaf 18) (rel2par consequence-s) (text _!Individual prosperity inevitably would result.<P>_!) )
                          )
                        )
                      )
                    )
                  )
                  ( Satellite (span 19 21) (rel2par consequence-s)
                    ( Nucleus (leaf 19) (rel2par span) (text _!That system has worked._!) )
                    ( Satellite (span 20 21) (rel2par evidence)
                      ( Nucleus (leaf 20) (rel2par List) (text _!The standard of living has increased steadily over the past 40 years;_!) )
                      ( Nucleus (leaf 21) (rel2par List) (text _!more than 90% of the people consider themselves middle class.<P>_!) )
                    )
                  )
                )
                ( Satellite (span 22 25) (rel2par elaboration-additional)
                  ( Satellite (span 22 23) (rel2par antithesis)
                    ( Nucleus (leaf 22) (rel2par span) (text _!The people have given their leading and only credible political party, the Liberal Democratic Party, clear and uninterrupted power for those 40 years._!) )
                    ( Satellite (leaf 23) (rel2par evidence) (text _!The LDP won by a landslide in the last election, in July 1986._!) )
                  )
                  ( Nucleus (span 24 25) (rel2par span)
                    ( Nucleus (leaf 24) (rel2par List) (text _!But less than two years later, the LDP started to crumble,_!) )
                    ( Nucleus (leaf 25) (rel2par List) (text _!and dissent rose to unprecedented heights.<P>_!) )
                  )
                )
              )
              ( Nucleus (span 26 27) (rel2par span)
                ( Nucleus (leaf 26) (rel2par span) (text _!The symptoms all point to one thing:_!) )
                ( Satellite (leaf 27) (rel2par elaboration-general-specific) (text _!Japan does not have a modern government._!) )
              )
            )
            ( Satellite (leaf 28) (rel2par example) (text _!Its government still wants to sit in the driver's seat, set the speed, step on the gas, apply the brakes and steer, with 120 million people in the back seat._!) )
          )
          ( Satellite (span 29 94) (rel2par elaboration-additional)
            ( Nucleus (span 29 63) (rel2par span)
              ( Nucleus (span 29 56) (rel2par span)
                ( Nucleus (span 29 33) (rel2par span)
                  ( Satellite (span 29 32) (rel2par concession)
                    ( Nucleus (span 29 31) (rel2par span)
                      ( Nucleus (leaf 29) (rel2par List) (text _!In a modern system, the government's role is to give the people as much choice as possible_!) )
                      ( Nucleus (span 30 31) (rel2par List)
                        ( Nucleus (leaf 30) (rel2par span) (text _!and to keep them well informed_!) )
                        ( Satellite (leaf 31) (rel2par enablement) (text _!so they are capable of making a choice._!) )
                      )
                    )
                    ( Satellite (leaf 32) (rel2par elaboration-additional) (text _!It also allows people to buy the best and the cheapest goods from anywhere in the world.<P>_!) )
                  )
                  ( Nucleus (leaf 33) (rel2par span) (text _!The Japanese government doesn't allow this._!) )
                )
                ( Satellite (span 34 56) (rel2par evidence)
                  ( Nucleus (leaf 34) (rel2par List) (text _!The Ministry of Agriculture and Fishery actually is a ministry for farmers and fishermen instead of a ministry of provisions._!) )
                  ( Nucleus (span 35 36) (rel2par List)
                    ( Nucleus (leaf 35) (rel2par span) (text _!The Ministry of Health and Welfare is a ministry of doctors and pharmaceutical companies rather than an organization_!) )
                    ( Satellite (leaf 36) (rel2par elaboration-object-attribute-e) (text _!dedicated to protecting the health of the people._!) )
                  )
                  ( Nucleus (span 37 38) (rel2par List)
                    ( Nucleus (leaf 37) (rel2par List) (text _!The Ministry of Education is nothing but a cartel for licensed teachers,_!) )
                    ( Nucleus (leaf 38) (rel2par List) (text _!and certainly does not act on behalf of students._!) )
                  )
                  ( Nucleus (span 39 44) (rel2par List)
                    ( Satellite (span 39 41) (rel2par antithesis)
                      ( Nucleus (leaf 39) (rel2par List) (text _!The Ministry of Construction spreads concrete throughout the country_!) )
                      ( Nucleus (span 40 41) (rel2par List)
                        ( Satellite (leaf 40) (rel2par attribution) (text _!and boasts in international conferences_!) )
                        ( Nucleus (leaf 41) (rel2par span) (text _!that Japan's paved roadway per capita is the longest in the world,_!) )
                      )
                    )
                    ( Nucleus (span 42 44) (rel2par span)
                      ( Nucleus (leaf 42) (rel2par span) (text _!but they seldom think of the poor commuters_!) )
                      ( Satellite (span 43 44) (rel2par elaboration-object-attribute-e)
                        ( Nucleus (leaf 43) (rel2par span) (text _!who spend so much time_!) )
                        ( Satellite (leaf 44) (rel2par circumstance) (text _!sitting in traffic._!) )
                      )
                    )
                  )
                  ( Nucleus (span 45 46) (rel2par List)
                    ( Nucleus (leaf 45) (rel2par span) (text _!The Ministry of Transportation serves the industry, certainly not the passengers_!) )
                    ( Satellite (leaf 46) (rel2par elaboration-object-attribute-e) (text _!who must pay extraordinarily high prices.<P>_!) )
                  )
                  ( Nucleus (span 47 56) (rel2par List)
                    ( Nucleus (span 47 51) (rel2par span)
                      ( Nucleus (leaf 47) (rel2par span) (text _!And the Ministry of Foreign Affairs works for itself,_!) )
                      ( Satellite (span 48 51) (rel2par manner)
                        ( Nucleus (leaf 48) (rel2par span) (text _!supporting Japanese diplomats_!) )
                        ( Satellite (span 49 51) (rel2par elaboration-object-attribute-e)
                          ( Nucleus (leaf 49) (rel2par span) (text _!who sprinkle abundant aid money around the world_!) )
                          ( Satellite (span 50 51) (rel2par purpose)
                            ( Satellite (leaf 50) (rel2par attribution) (text _!to ensure_!) )
                            ( Nucleus (leaf 51) (rel2par span) (text _!that their seat at the dinner table is next to the host's._!) )
                          )
                        )
                      )
                    )
                    ( Satellite (span 52 56) (rel2par elaboration-additional)
                      ( Nucleus (span 52 54) (rel2par span)
                        ( Nucleus (leaf 52) (rel2par span) (text _!This ministry has done nothing_!) )
                        ( Satellite (span 53 54) (rel2par elaboration-object-attribute-e)
                          ( Nucleus (leaf 53) (rel2par span) (text _!to correct the misunderstandings and misperceptions_!) )
                          ( Satellite (leaf 54) (rel2par elaboration-object-attribute-e) (text _!that are at the root of Japan's deteriorating image._!) )
                        )
                      )
                      ( Satellite (span 55 56) (rel2par elaboration-additional)
                        ( Nucleus (leaf 55) (rel2par span) (text _!Instead, it seems to be using foreign pressure and even the trade conflict_!) )
                        ( Satellite (leaf 56) (rel2par purpose) (text _!to expand its sphere of influence vis a vis other ministries.<P>_!) )
                      )
                    )
                  )
                )
              )
              ( Satellite (span 57 63) (rel2par elaboration-additional)
                ( Nucleus (span 57 59) (rel2par span)
                  ( Nucleus (span 57 58) (rel2par span)
                    ( Nucleus (leaf 57) (rel2par span) (text _!All this illustrates that Japanese ministries still have a "provider" mentality;_!) )
                    ( Satellite (leaf 58) (rel2par elaboration-additional) (text _!they do not serve the people, and particularly not consumers._!) )
                  )
                  ( Satellite (leaf 59) (rel2par elaboration-additional) (text _!They serve the industries and the special-interest groups._!) )
                )
                ( Satellite (span 60 63) (rel2par elaboration-additional)
                  ( Nucleus (span 60 61) (rel2par span)
                    ( Nucleus (leaf 60) (rel2par span) (text _!The rest of the world accepted such methods_!) )
                    ( Satellite (leaf 61) (rel2par circumstance) (text _!when Japan was developing._!) )
                  )
                  ( Satellite (span 62 63) (rel2par elaboration-additional)
                    ( Nucleus (leaf 62) (rel2par span) (text _!Japanese put up with it_!) )
                    ( Satellite (leaf 63) (rel2par reason) (text _!because the government provided job stability and growing paychecks.<P>_!) )
                  )
                )
              )
            )
            ( Satellite (span 64 94) (rel2par explanation-argumentative)
              ( Nucleus (span 64 67) (rel2par span)
                ( Nucleus (span 64 65) (rel2par span)
                  ( Satellite (leaf 64) (rel2par antithesis) (text _!Japan is not a political country._!) )
                  ( Nucleus (leaf 65) (rel2par span) (text _!It is a bureaucratic country._!) )
                )
                ( Satellite (span 66 67) (rel2par elaboration-additional)
                  ( Nucleus (leaf 66) (rel2par span) (text _!The Diet plays a minor role_!) )
                  ( Satellite (leaf 67) (rel2par elaboration-object-attribute-e) (text _!compared with the powerful bureaucratic system._!) )
                )
              )
              ( Satellite (span 68 94) (rel2par evidence)
                ( Nucleus (span 68 74) (rel2par span)
                  ( Nucleus (leaf 68) (rel2par List) (text _!Most bills are drafted by bureaucrats, not politicians._!) )
