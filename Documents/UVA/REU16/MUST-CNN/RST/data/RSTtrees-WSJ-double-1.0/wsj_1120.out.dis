                  )
                )
                ( Satellite (span 75 94) (rel2par example)
                  ( Nucleus (span 75 78) (rel2par span)
                    ( Nucleus (leaf 75) (rel2par span) (text _!A recent example is the 3% consumption tax on all commercial activities._!) )
                    ( Satellite (span 76 78) (rel2par evaluation-s)
                      ( Nucleus (leaf 76) (rel2par span) (text _!This makes enormous sense in Japan,_!) )
                      ( Satellite (span 77 78) (rel2par elaboration-object-attribute-e)
                        ( Nucleus (leaf 77) (rel2par List) (text _!where direct tax accounts for more than 70% of revenues_!) )
                        ( Nucleus (leaf 78) (rel2par List) (text _!and the capture rate of direct tax is so unfair._!) )
                      )
                    )
                  )
                  ( Satellite (span 79 94) (rel2par elaboration-additional)
                    ( Satellite (span 79 85) (rel2par concession)
                      ( Satellite (span 79 83) (rel2par background)
                        ( Nucleus (span 79 81) (rel2par Comparison)
                          ( Satellite (span 79 80) (rel2par condition)
                            ( Nucleus (leaf 79) (rel2par span) (text _!If you are a salaried man,_!) )
                            ( Satellite (leaf 80) (rel2par comment) (text _!Amen!_!) )
                          )
                          ( Nucleus (leaf 81) (rel2par span) (text _!100% captured._!) )
                        )
                        ( Nucleus (leaf 82) (rel2par Comparison) (text _!If you are a retailer, 50%,_!) )
                        ( Nucleus (leaf 83) (rel2par Comparison) (text _!and a farmer, 30%._!) )
                      )
                      ( Nucleus (span 84 85) (rel2par span)
                        ( Satellite (leaf 84) (rel2par purpose) (text _!To correct this inequality,_!) )
                        ( Nucleus (leaf 85) (rel2par span) (text _!most people would have favored an indirect tax, like the consumption tax._!) )
                      )
                    )
                    ( Nucleus (span 86 94) (rel2par span)
                      ( Satellite (span 86 87) (rel2par consequence-n)
                        ( Nucleus (leaf 86) (rel2par span) (text _!But the bill was passed_!) )
                        ( Satellite (leaf 87) (rel2par circumstance) (text _!without debate in the Diet, in the absence of the opposition._!) )
                      )
                      ( Nucleus (span 88 94) (rel2par span)
                        ( Nucleus (span 88 89) (rel2par span)
                          ( Nucleus (leaf 88) (rel2par span) (text _!As a result, the Japanese people didn't know what to expect_!) )
                          ( Satellite (leaf 89) (rel2par circumstance) (text _!when the new law was introduced on April 1._!) )
                        )
                        ( Satellite (span 90 94) (rel2par elaboration-additional)
                          ( Nucleus (span 90 91) (rel2par span)
                            ( Nucleus (leaf 90) (rel2par span) (text _!They were frustrated by the longer queues at the cashier and the small coins_!) )
                            ( Satellite (leaf 91) (rel2par elaboration-object-attribute-e) (text _!given as change._!) )
                          )
                          ( Satellite (span 92 94) (rel2par elaboration-additional)
                            ( Satellite (span 92 93) (rel2par result)
                              ( Satellite (leaf 92) (rel2par comparison) (text _!All of a sudden, prices were no longer in denominations of 100 or 200._!) )
                              ( Nucleus (leaf 93) (rel2par span) (text _!They were 103 or 206._!) )
                            )
                            ( Nucleus (leaf 94) (rel2par span) (text _!Pockets exploded with one-yen coins.<P>_!) )
                          )
                        )
                      )
                    )
