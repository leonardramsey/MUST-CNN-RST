        ( Satellite (span 95 108) (rel2par elaboration-additional)
          ( Satellite (span 95 96) (rel2par circumstance)
            ( Satellite (leaf 95) (rel2par temporal-same-time) (text _!While people were jingling their change,_!) )
            ( Nucleus (leaf 96) (rel2par span) (text _!the LDP politicians were caught in scandals._!) )
          )
          ( Nucleus (span 97 108) (rel2par span)
            ( Satellite (leaf 97) (rel2par background) (text _!Money, such as in Recruit's political donations, and women, as in the cases of Prime Minister Sosuke Uno and Secretary General Tokuo Yamashita, seldom have caused political scandals in Japan._!) )
            ( Nucleus (span 98 108) (rel2par span)
              ( Nucleus (span 98 101) (rel2par span)
                ( Satellite (span 98 99) (rel2par antithesis)
                  ( Nucleus (leaf 98) (rel2par span) (text _!Whereas most men were a bit ambivalent about the sex scandals_!) )
                  ( Satellite (leaf 99) (rel2par antithesis) (text _!(though they were furious about Recruit),_!) )
                )
                ( Nucleus (span 100 101) (rel2par span)
                  ( Satellite (leaf 100) (rel2par consequence-n) (text _!women were upset about both_!) )
                  ( Nucleus (leaf 101) (rel2par span) (text _!and surged to the polls._!) )
