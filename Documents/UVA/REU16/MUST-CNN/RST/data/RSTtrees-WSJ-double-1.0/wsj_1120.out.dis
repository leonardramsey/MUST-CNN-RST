                  )
                  ( Satellite (span 124 127) (rel2par elaboration-set-member)
                    ( Nucleus (span 124 125) (rel2par List)
                      ( Nucleus (leaf 124) (rel2par span) (text _!One party could stand for consumer interests, small government, free trade and globalism_!) )
                      ( Satellite (leaf 125) (rel2par consequence-s) (text _!to put Japan clearly among the most developed and open countries._!) )
                    )
                    ( Nucleus (span 126 127) (rel2par List)
                      ( Nucleus (leaf 126) (rel2par span) (text _!The other party could continue on the traditional track of the LDP,_!) )
                      ( Satellite (leaf 127) (rel2par elaboration-object-attribute-e) (text _!representing the manufacturers' preference for larger government, control, regulation and protectionism.<P>_!) )
                    )
