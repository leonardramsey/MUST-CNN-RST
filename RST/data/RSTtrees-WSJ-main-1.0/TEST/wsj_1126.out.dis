( Root (span 1 55)
  ( Nucleus (span 1 35) (rel2par span)
    ( Nucleus (span 1 13) (rel2par span)
      ( Nucleus (span 1 2) (rel2par span)
        ( Satellite (leaf 1) (rel2par attribution) (text _!Sheraton Corp. and Pan American World Airways announced_!) )
        ( Nucleus (leaf 2) (rel2par span) (text _!that they and two Soviet partners will construct two "world-class" hotels within a mile of Red Square in Moscow.<P>_!) )
      )
      ( Satellite (span 3 13) (rel2par summary-n)
        ( Nucleus (leaf 3) (rel2par span) (text _!U.S. and Soviet officials hailed the joint project as a new indication of the further thaw in U.S.-Soviet relations._!) )
        ( Satellite (span 4 13) (rel2par explanation-argumentative)
          ( Nucleus (span 4 7) (rel2par List)
            ( Nucleus (span 4 5) (rel2par span)
              ( Nucleus (leaf 4) (rel2par span) (text _!"This is an outstanding example_!) )
              ( Satellite (leaf 5) (rel2par elaboration-object-attribute-e) (text _!of how the East and the West can work together for their mutual benefit and progress,"_!) )
            )
            ( Satellite (span 6 7) (rel2par attribution)
              ( Nucleus (leaf 6) (rel2par span) (text _!said Soviet Ambassador Yuri Dubinin,_!) )
              ( Satellite (leaf 7) (rel2par elaboration-additional) (text _!who hosted a signing ceremony for the venture's partners at the Soviet embassy here.<P>_!) )
            )
          )
          ( Nucleus (span 8 13) (rel2par List)
            ( Nucleus (span 8 10) (rel2par span)
              ( Nucleus (span 8 9) (rel2par Same-Unit)
                ( Nucleus (leaf 8) (rel2par span) (text _!Commerce Secretary Robert Mosbacher,_!) )
                ( Satellite (leaf 9) (rel2par elaboration-additional-e) (text _!who attended the ceremony,_!) )
              )
              ( Nucleus (leaf 10) (rel2par Same-Unit) (text _!called the undertaking a "historic step" in the evolution of U.S.-Soviet ties._!) )
            )
            ( Satellite (span 11 13) (rel2par elaboration-additional)
              ( Satellite (leaf 11) (rel2par attribution) (text _!He added_!) )
              ( Nucleus (span 12 13) (rel2par span)
                ( Nucleus (leaf 12) (rel2par span) (text _!that it likely will have a "mulitiplier effect"_!) )
                ( Satellite (leaf 13) (rel2par elaboration-object-attribute-e) (text _!in stimulating further trade between the two countries.<P>_!) )
              )
            )
          )
        )
      )
    )
    ( Satellite (span 14 35) (rel2par elaboration-additional)
      ( Nucleus (span 14 15) (rel2par span)
        ( Nucleus (leaf 14) (rel2par span) (text _!The project will be the largest U.S.-backed joint venture_!) )
        ( Satellite (leaf 15) (rel2par elaboration-object-attribute-e) (text _!to be undertaken in the Soviet Union in recent years._!) )
      )
      ( Satellite (span 16 35) (rel2par elaboration-additional)
        ( Nucleus (span 16 29) (rel2par span)
          ( Nucleus (span 16 25) (rel2par List)
            ( Nucleus (span 16 20) (rel2par span)
              ( Nucleus (span 16 17) (rel2par Same-Unit)
                ( Nucleus (leaf 16) (rel2par span) (text _!One of the hotels,_!) )
                ( Satellite (leaf 17) (rel2par elaboration-additional-e) (text _!to be called the Sheraton Moscow,_!) )
              )
              ( Nucleus (span 18 20) (rel2par Same-Unit)
                ( Nucleus (leaf 18) (rel2par List) (text _!will have 450 rooms_!) )
                ( Nucleus (span 19 20) (rel2par List)
                  ( Nucleus (leaf 19) (rel2par span) (text _!and will cost an estimated $75 million_!) )
                  ( Satellite (leaf 20) (rel2par purpose) (text _!to build._!) )
                )
              )
            )
            ( Satellite (span 21 25) (rel2par elaboration-additional)
              ( Nucleus (leaf 21) (rel2par List) (text _!The six-story hotel will be on Gorky Street_!) )
              ( Nucleus (leaf 22) (rel2par List) (text _!and initially will cater mostly to business travelers._!) )
              ( Nucleus (span 23 24) (rel2par List)
                ( Nucleus (leaf 23) (rel2par span) (text _!It will have a Russian tavern, an English pub, a discotheque and Japanese and Italian restaurants,_!) )
                ( Satellite (leaf 24) (rel2par attribution) (text _!according to a Sheraton announcement._!) )
              )
              ( Nucleus (leaf 25) (rel2par List) (text _!The hotel is scheduled to open in 1992.<P>_!) )
            )
          )
          ( Nucleus (span 26 29) (rel2par List)
            ( Nucleus (span 26 28) (rel2par span)
              ( Nucleus (span 26 27) (rel2par Same-Unit)
                ( Nucleus (leaf 26) (rel2par span) (text _!The second hotel,_!) )
                ( Satellite (leaf 27) (rel2par elaboration-additional-e) (text _!to be called the Budapest Hotel,_!) )
              )
              ( Nucleus (leaf 28) (rel2par Same-Unit) (text _!is to be constructed at a site even closer to Red Square._!) )
            )
            ( Satellite (leaf 29) (rel2par elaboration-additional) (text _!Details about its size and cost haven't yet been determined.<P>_!) )
          )
        )
        ( Satellite (span 30 35) (rel2par elaboration-additional)
          ( Nucleus (span 30 31) (rel2par List)
            ( Nucleus (leaf 30) (rel2par List) (text _!Sheraton, a subsidiary of ITT Corp., will have a 40% share in the two hotels;_!) )
            ( Nucleus (leaf 31) (rel2par List) (text _!Pan American, a subsidiary of Pan Am Corp., will have a 10% share._!) )
          )
          ( Nucleus (leaf 32) (rel2par List) (text _!The Soviet owners will be Mossoviet, Moscow's city governing body, and Aeroflot, the Soviet national airline.<P>_!) )
          ( Nucleus (span 33 35) (rel2par List)
            ( Satellite (leaf 33) (rel2par concession) (text _!Although a Finnish group has a minority interest in an already operating Moscow hotel,_!) )
            ( Nucleus (span 34 35) (rel2par span)
              ( Nucleus (leaf 34) (rel2par span) (text _!the Sheraton-Pan Am venture will be the first joint-venture hotels in the Soviet Union_!) )
              ( Satellite (leaf 35) (rel2par elaboration-object-attribute-e) (text _!to have as much as 50% foreign ownership.<P>_!) )
            )
          )
        )
      )
    )
  )
  ( Satellite (span 36 55) (rel2par background)
    ( Satellite (span 36 38) (rel2par concession)
      ( Nucleus (leaf 36) (rel2par span) (text _!U.S. companies account for less than 8% of the 1,000 or more Soviet joint ventures_!) )
      ( Satellite (span 37 38) (rel2par elaboration-object-attribute-e)
        ( Nucleus (leaf 37) (rel2par span) (text _!that have been announced_!) )
        ( Satellite (leaf 38) (rel2par circumstance) (text _!since the Soviets began encouraging such undertakings in 1987._!) )
      )
    )
    ( Nucleus (span 39 55) (rel2par span)
      ( Nucleus (span 39 41) (rel2par span)
        ( Nucleus (leaf 39) (rel2par span) (text _!But some U.S. companies are negotiating projects_!) )
        ( Satellite (span 40 41) (rel2par elaboration-additional)
          ( Nucleus (leaf 40) (rel2par span) (text _!that could be among the biggest ones_!) )
          ( Satellite (leaf 41) (rel2par elaboration-object-attribute-e) (text _!to be launched._!) )
        )
      )
      ( Satellite (span 42 55) (rel2par example)
        ( Nucleus (span 42 43) (rel2par List)
          ( Nucleus (leaf 42) (rel2par span) (text _!Chevron Corp., Amoco Corp., Archer-Daniels-Midland Co., and Eastman Kodak Co. are among the U.S. companies_!) )
          ( Satellite (leaf 43) (rel2par elaboration-object-attribute-e) (text _!known to be considering such ventures.<P>_!) )
        )
        ( Nucleus (span 44 55) (rel2par List)
          ( Nucleus (span 44 50) (rel2par span)
            ( Nucleus (span 44 46) (rel2par span)
              ( Satellite (leaf 44) (rel2par attribution) (text _!Sheraton and Pan Am said_!) )
              ( Nucleus (span 45 46) (rel2par span)
                ( Satellite (leaf 45) (rel2par attribution) (text _!they are assured under the Soviet joint-venture law_!) )
                ( Nucleus (leaf 46) (rel2par span) (text _!that they can repatriate profits from their hotel venture._!) )
              )
            )
            ( Satellite (span 47 50) (rel2par explanation-argumentative)
              ( Nucleus (span 47 49) (rel2par span)
                ( Nucleus (leaf 47) (rel2par List) (text _!The Sheraton Moscow will charge about $140 to $150 a day for each of its rooms,_!) )
                ( Nucleus (span 48 49) (rel2par List)
                  ( Nucleus (leaf 48) (rel2par span) (text _!and it will accept payment only in currencies_!) )
                  ( Satellite (leaf 49) (rel2par elaboration-object-attribute-e) (text _!that can be traded in foreign exchange markets,_!) )
                )
              )
              ( Satellite (leaf 50) (rel2par attribution) (text _!according to a Sheraton executive.<P>_!) )
            )
          )
          ( Satellite (span 51 55) (rel2par elaboration-additional)
            ( Nucleus (span 51 53) (rel2par List)
              ( Satellite (leaf 51) (rel2par attribution) (text _!Thomas Plaskett, Pan Am's chairman, said_!) )
              ( Nucleus (span 52 53) (rel2par span)
                ( Nucleus (leaf 52) (rel2par span) (text _!the U.S. airline's participation is a natural outgrowth of its current arrangements with Aeroflot_!) )
                ( Satellite (leaf 53) (rel2par elaboration-object-attribute-e) (text _!to jointly operate nonstop New York-Moscow flights._!) )
              )
            )
            ( Nucleus (span 54 55) (rel2par List)
              ( Satellite (leaf 54) (rel2par attribution) (text _!He said_!) )
              ( Nucleus (leaf 55) (rel2par span) (text _!the rising volume of passenger traffic on this route justifies a major investment in new high-standard Moscow hotels._!) )
            )
          )
        )
      )
    )
  )
)
